            <Button x:Name="productionPlanButton" Content="Button" Click="Button_Click" RenderTransformOrigin="-0.138,2.348" HorizontalAlignment="Right" Width="90" Height="412"/>
        </StackPanel>
    </DockPanel>

</Window>
