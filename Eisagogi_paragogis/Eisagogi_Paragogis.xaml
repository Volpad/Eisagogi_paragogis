        <ContextMenu x:Key="cmenu" Tag="cmenu">
            <!--<MenuItem Header="Put On Machine" Click="Click_OnMachine"/>-->
            <!--<MenuItem Header="Check Balance" Click="Balance_Click"/>-->
            <!--<MenuItem Header="Copy Access No" Click="Click_Copy_Access_Number"/>
