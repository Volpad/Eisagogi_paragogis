                    <StackPanel x:Name="SM55" Grid.Column="0"  VerticalAlignment="Center"  HorizontalAlignment="Center" Width="132" Height="130" Margin="0,5,0,0" Background="#e0e0e0">

                        <Canvas x:Name="M55" />
                        <DataGrid x:Name="D55" AutoGenerateColumns="False" Margin="0,25,0,0" Width="132" FontFamily="Calibri (Detail)"  Background="White" AlternationCount="2" HorizontalAlignment="Left" MouseDoubleClick="Grid_MouseDoubleClick" >
                            <DataGrid.Resources>
                                <SolidColorBrush x:Key="{x:Static SystemColors.HighlightBrushKey}" Color="#FFE9E2FF"/>
                            </DataGrid.Resources>
                            <DataGrid.Columns>
                                <DataGridTextColumn Binding="{Binding Path = Name}" FontSize="10"/>
                                <DataGridTextColumn Binding="{Binding Path = TotalId}" FontSize="10"/>
                                <DataGridTextColumn Binding="{Binding Path = Rest}" FontSize="8"/>
                            </DataGrid.Columns>
                                <DataGrid.CellStyle>

                                <Style TargetType="DataGridCell">

                                    <Setter Property="BorderBrush" Value="#d0cece" />
                                    <Setter Property="BorderThickness" Value="1 1 1 1" />
                                    <Setter Property="Margin" Value="1,0,1,0" />
                                    <Style.Triggers>
                                        <Trigger Property="IsSelected" Value="True">
                                            <Setter Property="Foreground" Value="black"/>
                                        </Trigger>
                                    </Style.Triggers>
                                </Style>

                            </DataGrid.CellStyle>


                            <DataGrid.RowStyle>


                                <Style TargetType="{x:Type DataGridRow}">
                                    <Setter Property="Margin" Value="0,2,0,2" />
                                    <!--<EventSetter Event="MouseDoubleClick" Handler="Row_DoubleClick" />-->
                                    <Style.Triggers>

                                        <Trigger Property="ItemsControl.AlternationIndex" Value="0">
                                            <Setter Property="Background" Value="white"/>

                                        </Trigger>
                                        <Trigger Property="ItemsControl.AlternationIndex" Value="1">
                                            <Setter Property="Background" Value="#f2f2f2"/>
                                        </Trigger>
                                    </Style.Triggers>
                                </Style>
                            </DataGrid.RowStyle>
                        </DataGrid>

